{
    "env": {
      "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks", "prettier"],
    "rules": {
      "prettier/prettier": "warn",
      "no-undef": "off"
      //add customize rules here as per your project's needs
    }
}  